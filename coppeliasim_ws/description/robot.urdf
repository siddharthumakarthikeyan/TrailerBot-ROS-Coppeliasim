<?xml version="1.0" ?>
<robot name="differential_wheeled_robot"
    xmlns:xacro="http://www.ros.org/wiki/xacro">
    <material name="Black">
        <color rgba="0.0 0.0 0.0 1.0"/>
    </material>
    <material name="Red">
        <color rgba="0.8 0.0 0.0 1.0"/>
    </material>
    <material name="White">
        <color rgba="1.0 1.0 1.0 1.0"/>
    </material>
    <material name="Blue">
        <color rgba="0.0 0.0 0.8 1.0"/>
    </material>

    <link name="base_footprint">
        <inertial>
            <mass value="0.0001"/>
            <origin xyz="0 0 0"/>
            <inertia ixx="0.0001" ixy="0.0" ixz="0.0" iyy="0.0001" iyz="0.0" izz="0.0001"/>
        </inertial>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0.2"/>
            <geometry>
                <mesh filename="/home/siddharth/Desktop/base_link.stl" scale="0.01 0.01 0.01"/>
            </geometry>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0.2"/>
            <geometry>
                <mesh filename="/home/siddharth/Desktop/base_link.stl" scale="0.01 0.01 0.01"/>
            </geometry>
        </collision>
    </link>

    <joint name="left_Wheel_joint" type="revolute">
        <origin rpy="1.57 0 0" xyz="-0.575 -0.575 0.2"/>
        <axis xyz="0 0 1"/>
        <parent link="base_footprint"/>
        <child link="left_wheel"/>
    </joint>

    <link name="left_wheel">
        <inertial>
            <mass value="0.0001"/>
            <origin xyz="0 0 0"/>
            <inertia ixx="0.0001" ixy="0.0" ixz="0.0" iyy="0.0001" iyz="0.0" izz="0.0001"/>
        </inertial>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder radius="0.2" length="0.1"/>
            </geometry>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder radius="0.2" length="0.1"/>
            </geometry>
        </collision>
    </link>

    <joint name="right_Wheel_joint" type="revolute">
        <origin rpy="1.57 0 0" xyz="-0.575 0.575 0.2"/>
        <axis xyz="0 0 1"/>
        <parent link="base_footprint"/>
        <child link="right_wheel"/>
    </joint>

    <link name="right_wheel">
        <inertial>
            <mass value="0.0001"/>
            <origin xyz="0 0 0"/>
            <inertia ixx="0.0001" ixy="0.0" ixz="0.0" iyy="0.0001" iyz="0.0" izz="0.0001"/>
        </inertial>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder radius="0.2" length="0.1"/>
            </geometry>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder radius="0.2" length="0.1"/>
            </geometry>
        </collision>
    </link>


    <!--  -->

    <joint name="cart_joint" type="revolute">
        <origin rpy="0 0 0" xyz="-1.2 0.0 0.8"/>
        <axis xyz="0 0 1"/>
        <parent link="base_footprint"/>
        <child link="cart_body"/>
    </joint>

    <link name="cart_body">
        <inertial>
            <mass value="0.0001"/>
            <origin xyz="0 0 0"/>
            <inertia ixx="0.0001" ixy="0.0" ixz="0.0" iyy="0.0001" iyz="0.0" izz="0.0001"/>
        </inertial>
        <visual>
            <origin rpy="0 0 -1.57" xyz="-1.1 -0.0 -0.6"/>
            <geometry>
                <mesh filename="/home/siddharth/Desktop/cart.stl" scale="0.01 0.01 0.01"/>
            </geometry>
        </visual>
        <collision>
            <origin rpy="0 0 -1.57" xyz="-1.2 -0.0 -0.6"/>
            <geometry>
                <mesh filename="/home/siddharth/Desktop/cart.stl" scale="0.01 0.01 0.01"/>
            </geometry>
        </collision>
    </link>

</robot>
